package optimus.v1;

/**
 * 
 * @version 1.0
 * @author Lucas
 *
 */
public class Shoot {
	
	private int size;
	private double power;
	
	public Shoot(int size, double power){
		this.power = power;
		this.size = size;
	}

	public int getSize() {
		return size;
	}

	public void setSize(int size) {
		this.size = size;
	}

	public double getPower() {
		return power;
	}

	public void setPower(double power) {
		this.power = power;
	}
	
}
